{
  "name": "server",
  "version": "2.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-transform-types ./src/main.ts",
    "dev": "npm run start",
    "build": "tsc -p tsconfig.json",
    "gen": "graphql-codegen --config codegen.config.ts",
    "migrate": "node --experimental-transform-types ./src/migrate.ts",
    "deploy": "node --experimental-transform-types ./deploy.ts"
  },
  "dependencies": {
    "db": "workspace:*",
    "domain-model": "workspace:*",
    "aws-sdk": "^2.1692.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "~4.16.1",
    "express-validator": "^6.12.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "http-errors": "~2.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^22.15.21",
    "@types/node-fetch": "^2.6.12",
    "nodemon": "^3.1.10"
  }
}
