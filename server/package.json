    "class-validator": "^0.14.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv": "^6.3.11",
    "dotenv": "^16.5.0",
    "express": "~5.1.0",
    "express-validator": "^7.2.1",
    "find-config": "^1.0.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "http-errors": "~2.0.0",
    "http-status-codes": "^2.3.0",
