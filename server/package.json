{
  "name": "server",
  "version": "2.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "fmt": "biome format --write",
    "lint": "biome lint --write",
    "fix": "biome check --write",
    "start": "bun run ./src/main.ts",
    "dev": "bun run --watch ./src/main.ts",
    "build": "tsc -p tsconfig.json",
    "check": "tsc -p tsconfig.json --noEmit",
    "test": "bun test",
    "deploy": "bun run ./deploy.ts"
  },
  "imports": {
    "#root/*": "./src/*"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.1000.0",
    "@aws-sdk/lib-storage": "^3.1000.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^9.0.0",
    "@trpc/server": "^11.4.2",
    "db": "workspace:*",
    "decimal.js": "^10.6.0",
    "fastify": "^5.7.4",
    "http-errors": "~2.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "kysely": "^0.28.8",
    "models": "workspace:*",
    "moment": "^2.30.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "rust": "workspace:*",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "aws-sdk"
  ]
}
