{
  "name": "server",
  "version": "2.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "fmt": "biome format --write",
    "lint": "biome lint --write",
    "fix": "biome check --write",
    "start": "bun run ./src/main.ts",
    "dev": "bun run --watch ./src/main.ts",
    "build": "tsc -p tsconfig.json",
    "check": "tsc -p tsconfig.json --noEmit",
    "test": "bun test",
    "deploy": "bun run ./deploy.ts"
  },
  "imports": {
    "#root/*": "./src/*"
  },
  "dependencies": {
    "@trpc/server": "^11.4.2",
    "aws-sdk": "^2.1692.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db": "workspace:*",
    "rust": "workspace:*",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.6.1",
    "express": "~4.16.1",
    "express-validator": "^6.12.0",
    "http-errors": "~2.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "kysely": "^0.28.8",
    "models": "workspace:*",
    "moment": "^2.30.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "safe-wrapper": "^3.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "aws-sdk"
  ]
