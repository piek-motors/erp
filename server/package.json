{
  "name": "server",
  "version": "2.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-transform-types ./src/main.ts",
    "dev": "npm run start",
    "build": "tsc -p tsconfig.json",
    "gen": "graphql-codegen --config codegen.config.ts",
